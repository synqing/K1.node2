{
  "name": "Audio Test - Spectrum Bin",
  "description": "Simple test pattern using spectrum_bin (bass frequency) to control brightness",
  "palette_data": [
    [0, 0, 0, 50],
    [128, 100, 50, 200],
    [255, 255, 100, 255]
  ],
  "nodes": [
    {
      "id": "bass_spectrum",
      "type": "spectrum_bin",
      "description": "Read bass frequency bin (bin 5 = ~110Hz)",
      "parameters": {
        "bin": 5
      }
    },
    {
      "id": "palette_output",
      "type": "palette_interpolate",
      "description": "Map bass level to color palette",
      "inputs": ["bass_spectrum"],
      "parameters": {
        "palette": "audio_reactive_blue"
      }
    },
    {
      "id": "final_output",
      "type": "output",
      "description": "Final LED output"
    }
  ],
  "wires": [
    {
      "from": "bass_spectrum",
      "to": "palette_output",
      "description": "Bass level drives palette position"
    },
    {
      "from": "palette_output",
      "to": "final_output",
      "description": "Colored output to LEDs"
    }
  ]
}
