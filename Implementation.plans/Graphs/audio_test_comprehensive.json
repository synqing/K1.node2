{
  "name": "Audio Test - Comprehensive",
  "description": "Test multiple audio node types: spectrum_range (bass average) and audio_level",
  "palette_data": [
    [0, 0, 0, 0],
    [64, 255, 0, 0],
    [128, 255, 255, 0],
    [192, 0, 255, 0],
    [255, 0, 0, 255]
  ],
  "nodes": [
    {
      "id": "bass_range",
      "type": "spectrum_range",
      "description": "Average bass frequencies (bins 3-8 = 55-170Hz)",
      "parameters": {
        "start_bin": 3,
        "end_bin": 8
      }
    },
    {
      "id": "overall_level",
      "type": "audio_level",
      "description": "Overall VU level"
    },
    {
      "id": "combined",
      "type": "multiply",
      "description": "Combine bass with overall level",
      "inputs": ["bass_range", "overall_level"]
    },
    {
      "id": "palette_output",
      "type": "palette_interpolate",
      "description": "Map to rainbow palette",
      "inputs": ["combined"],
      "parameters": {
        "palette": "audio_reactive_rainbow"
      }
    },
    {
      "id": "final_output",
      "type": "output",
      "description": "Final LED output"
    }
  ],
  "wires": [
    {
      "from": "bass_range",
      "to": "combined"
    },
    {
      "from": "overall_level",
      "to": "combined"
    },
    {
      "from": "combined",
      "to": "palette_output"
    },
    {
      "from": "palette_output",
      "to": "final_output"
    }
  ]
}
