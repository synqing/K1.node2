{
  "name": "@k1/host-suite",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "qa": "bash ./scripts/qa-pipeline.sh",
    "qa:graph": "tsx ./agents/graph-theorist.ts",
    "qa:deps": "tsx ./agents/dep-cartographer.ts",
    "qa:estimate": "tsx ./agents/perf-oracle.ts",
    "qa:validate": "tsx ./agents/semantic-arbiter.ts",
    "qa:path": "tsx ./agents/path-ranger.ts",
    "qa:ast": "tsx ./agents/cpp-sentry.ts && tsx ./agents/ast-surgeon.ts",
    "qa:profile": "tsx ./agents/profiler.ts",
    "qa:exec": "tsx ./agents/graph-executor.ts",
    "qa:audio": "tsx ./agents/audio-sim-lab.ts",
    "qa:visual": "tsx ./agents/visual-examiner.ts",
    "qa:perfstats": "tsx ./agents/perf-analyst.ts"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "typescript": "^5.6.3",
    "tsx": "^4.16.2",
    "vitest": "^2.1.3",
    "tree-sitter": "^0.22.5",
    "tree-sitter-cpp": "^0.20.3",
    "pngjs": "^7.0.0"
  }
}